# AWS ElasticSearch Endpoint
# Works regardless of the trailing slash /
# On localhost docker deployment use: 
# ELASTICSEARCH_URL = "http://hubmap-elasticsearch:9200"
ELASTICSEARCH_URL = ''

# Default the index name to 'entities' for `/search`
DEFAULT_INDEX = 'entities'

# Naming convention of indices (always in pair) in Elasticsearch
PUBLIC_INDEX_PREFIX = 'hm_public_'
PRIVATE_INDEX_PREFIX = 'hm_consortium_'

# Entity-API service
# Default value works with docker deployment on localhost, dev, test, and prod
# Don't use the trailing slash /
ENTITY_WEBSERVICE_URL = 'http://hubmap-auth:3333'

# Globus app client ID and secret
APP_CLIENT_ID = ''
APP_CLIENT_SECRET = ''

# Globus Hubmap-Read group UUID
GLOBUS_HUBMAP_READ_GROUP_UUID = '5777527e-ec11-11e8-ab41-0af86edb4424'
